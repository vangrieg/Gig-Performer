On Deactivate
    osc_enabled = false
End

On Activate
    osc_enabled = true
End

// Called when you switch variations
On Variation(oldVariation : integer, newVariation : integer)
    OSC_SendIntegerSpecific ("/ping", initmode, oscip, oscport)
End